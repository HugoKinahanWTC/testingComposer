{
    "name": "hkinahan/testing-composer",
    "description": "Challenge: Create a git repo with access to two different versions of index.html using Composer.",
    "type": "project",
    "license": "MIT",
    "version": "v1.0",
    "authors": [
        {
            "name": "Hugo Kinahan",
            "email": "hugo@example.com"
        }
    ],
    "repositories": [
        {
        "type": "package", 
        "package": {
            "name": "hkinahan/version1", 
            "version": "1.0",
            "source": {
                "url": "https://github.com/HugoKinahanWTC/index-html-version-1.git", 
                "type": "git", 
                "reference": "main"
            }
        }
    },
    {
        "type": "package", 
        "package": {
            "name": "hkinahan/version2", 
            "version": "1.0",
            "source": {
                "url": "https://github.com/HugoKinahanWTC/index-html-version-2.git", 
                "type": "git", 
                "reference": "main"
            }
        }
    }
],
    "require": {
        "nesbot/carbon": "2.55.2",
        "vendor/index.html": "dev-dev/#v2.0"
    }
}
